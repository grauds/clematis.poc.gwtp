<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
    ui:generateLocales="default">
    
<ui:style type="org.clematis.web.elearning.client.ui.MenuListItemWidget.MenuListItemStyle">
   
    .group
    {
	    margin: 15px;
	    padding: 20px;
	   	border: 1px solid #323232;
	   	text-align: left;
	   	width: 300px;
	}
	
	.links
	{
	    padding: 5px;	
	}
   
    .cell
    {
        margin-top: 20px;  	   
        margin-bottom: 20px;
        margin-right: 20px;
        margin-left: 10px;
	}
	
	.linkText
	{
	    font-size: 14px;
	    color: #000000;	
	    padding: 2px;
		margin-left: 5px;
	    border: 0px;
        text-decoration:none;
	}	
	
	.link
	{
	    font-size: 14px;
	    color: #3671d1;	
	    padding: 2px;
	    border: 0px;
	    cursor: pointer;
        cursor: hand;
        text-decoration:none;
	}
	
	.link a:link 
	{
		color:#3671d1; 
		text-decoration:none;
    }  

	.link a:visited 
	{
		color:#3671d1; 
		text-decoration:none;
    }  
	
	.link a:hover 
	{
		color:#3671d1; 
		text-decoration:none;
    }  

	.link a:active 
	{
		color:#3671d1; 
		text-decoration:none;
    }  
	

</ui:style>


<g:HTMLPanel styleName="{style.group}">
    <div style="padding: 5px; padding-bottom: 10px;">
        <g:Label styleName="blackHeader" ui:field="caption" />
    </div>
    <g:VerticalPanel styleName="links" ui:field="links" />
</g:HTMLPanel>		            

</ui:UiBinder>